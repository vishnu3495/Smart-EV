<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="connector_details_tree" model="ir.ui.view">
        <field name="name">connector.details.tree</field>
        <field name="model">connect.details</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="connector_details_form" model="ir.ui.view">
        <field name="name">connector.details.form</field>
        <field name="model">connect.details</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="electric_vehicle_action" model="ir.actions.act_window">
        <field name="name">Connector</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">connect.details</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>